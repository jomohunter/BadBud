<div class="rn-breadcrumb-inner ptb--30">
    <div class="container">

        
            
            <div class="col-lg-6 col-md-6 col-12">
                <ul class="breadcrumb-list">
                    <li class="item"><a href="index.html">Home</a></li>
                    <li class="separator"><i class="feather-chevron-right"></i></li>
                    <li class="item current">Collection</li>
                </ul>
            </div>

            <div class="row align-items-center">
                <div class="col-lg-6 col-md-6 col-12" style="padding-bottom: 20px;" >
                    <h6 class="title text-center text-md-start" style="padding-bottom: 20px;">{{ project.nom }}</h6>
                    <p class="description">{{ project.description }}</p>
                    <p class="category font-weight-bold">{{ project.category }}</p>
                    <p class="date" style="font-size: 0.8rem; color: #666;"> Created: {{ project.DateDeCreation ? project.DateDeCreation|date('Y-m-d') : '' }}</p>
                    <a href="{{ path('edit_projects', {'id': project.id}) }}" class="btn btn-primary " >Edit</a>
                    
                    <form action="{{ path('delete_projet', {'id': project.id}) }}"   method="post" onsubmit="return confirm('Are you sure you want to delete this project?')">
                        <input type="hidden" name="_method" value="DELETE">
                        <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ project.id) }}">
                        <button type="submit" class="btn btn-danger">Delete</button>
                    </form>
                     style="padding-bottom: 20px;"
                </div>
            </div>
        </div>
    </div>
</div>

